(in-package #:cl-user)

(asdf:oos 'asdf:load-op 'asdf-install)

;; Install prerequisites
(asdf-install:install 'cffi)
(asdf-install:install 's-sysdeps)
(asdf-install:install 'cl-fad)
(asdf-install:install 'tinaa)
(asdf-install:install 'fiveam)

;; Uninstall prerequisites
(asdf-install:uninstall 'cffi)
(asdf-install:uninstall 's-sysdeps)
(asdf-install:uninstall 'cl-fad)
(asdf-install:uninstall 'tinaa)
(asdf-install:uninstall 'fiveam)
